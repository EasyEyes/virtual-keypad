    "compilerOptions": {
      "outDir": "./js",
      "noImplicitAny": true,
      "module": "es6",
      "target": "es5",
      "allowJs": true,
      "moduleResolution": "node",
      "allowSyntheticDefaultImports": true,
      "downlevelIteration": true,
      "esModuleInterop": true,
      "sourceMap": true,
    }
